{
  "name": "service-users",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "node build/src/index.js",
    "dev": "dotenv -e .env.local -- ts-node src/index.ts",
    "build": "tsc",
    "build:ci": "npm run prisma:generate && npm run prisma:migrate && npm run build",
    "stripe:dev": "stripe listen --forward-to  http://localhost:3000/api/stripe/webhook",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "prisma:seed": "dotenv -e .env.local -- prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@stripe/stripe-js": "^1.48.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "express": "^4.18.2",
    "kafka-node": "^5.0.0",
    "kafkajs": "^2.2.4",
    "prisma": "^4.11.0",
    "stripe": "^11.13.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.2"
  }
}
