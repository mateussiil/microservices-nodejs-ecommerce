{
  "name": "service-users",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "npm run build:ci && node build/src/index.js",
    "dev": "dotenv -e .env.local -- ts-node src/index.ts",
    "build": "tsc",
    "build:ci": "npm run prisma:generate:prod && npm run prisma:migrate:prod && npm run build",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:generate:prod": "prisma generate",
    "prisma:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:seed": "dotenv -e .env.local -- prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@types/jsonwebtoken": "^9.0.1",
    "cookie-session": "^2.0.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "kafka-node": "^5.0.0",
    "kafkajs": "^2.2.4",
    "prisma": "^4.11.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@types/cookie-session": "^2.0.44",
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.2",
    "@types/yup": "^0.32.0"
  }
}
